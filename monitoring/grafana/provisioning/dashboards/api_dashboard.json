{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "title": "API Performance Dashboard",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "id": 10,
      "title": "API Request Rate (by handler)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        { "expr": "sum by (handler)(rate(http_requests_total[1m]))", "legendFormat": "{{handler}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] }
    },
    {
      "id": 20,
      "title": "API Request Latency (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum by (le)(rate(http_request_duration_seconds_bucket[5m])))", "legendFormat": "p95" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },
    {
      "id": 30,
      "title": "Error Rate (5xx)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        { "expr": "sum(rate(http_requests_total{status=~\"5..\"}[1m]))", "legendFormat": "5xx/s" }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] }
    },
    {
      "id": 40,
      "title": "Predicted House Value â€“ Mean (5m)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        { "expr": "sum(increase(predicted_house_value_sum[5m])) / sum(increase(predicted_house_value_count[5m]))", "legendFormat": "mean" }
      ]
    },
    {
      "id": 50,
      "title": "Predicted House Value Distribution",
      "type": "histogram",
      "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        { "expr": "sum by (le)(rate(predicted_house_value_bucket[5m]))", "legendFormat": "{{le}}" }
      ]
    }
  ],
  "time": { "from": "now-1h", "to": "now" }
}
